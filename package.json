{
  "version": "0.2.9-beta",
  "name": "@neptunemutual/sdk",
  "author": "Neptune Mutual Team",
  "description": "Neptune Mutual SDK",
  "license": "MIT",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "source": "src/index.ts",
  "exports": {
    "default": "./dist/index.js"
  },
  "type": "module",
  "module": "./dist/index.js",
  "scripts": {
    "build": "rm -rf ./dist && microbundle -i ./src/index.ts -o ./dist/index.js --target web -f esm --generateTypes true",
    "compile": "yarn build",
    "dev": "microbundle watch",
    "standard": "ts-standard",
    "fix": "ts-standard --fix",
    "publish": "npm run build && npm publish"
  },
  "dependencies": {
    "bs58": "4.0.1",
    "ethers": "5.4.7",
    "ethers-multicall": "^0.2.2",
    "ipfs-mini": "1.1.5",
    "lru-cache": "6.0.0"
  },
  "devDependencies": {
    "@types/bs58": "4.0.1",
    "@types/lru-cache": "5.1.1",
    "microbundle": "^0.14.2",
    "ts-standard": "11.0.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neptune-mutual/sdk.git"
  },
  "bugs": {
    "url": "https://github.com/neptune-mutual/sdk/issues"
  },
  "homepage": "https://github.com/neptune-mutual/sdk#readme"
}